package basicjava;
import javax.swing.*;

public class Application {
	public static void main(String[] args){
		
		JFrame f = new JFrame("Calculator");
		f.setSize(1000, 1000);
		f.setVisible(true);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		//Creating a # panel
		JPanel p = new JPanel();
		p.setSize(400, 600);
		p.setVisible(true);
		p.
	}
}
