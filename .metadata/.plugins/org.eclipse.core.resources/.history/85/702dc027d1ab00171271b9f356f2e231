public class Person {
	
	// PROPERTIES - START
	private int age;
	private String name;
	private int grade;
	// PROPERTIES - END

	// CONSTRUCTORS - START
	// constructors parameters must be only ones necessary to create the object
	Person(int years, String letters, int standard){
		age= years;
		name= letters;
		grade= standard;		
	}
	// CONSTRUCTORS - END
	
	// METHODS
	int getAge(){
		return age;
	}
	String getName(){
		return name;
	}
	int getGrade(){
		return grade;
	}


}


