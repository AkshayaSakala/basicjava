
public class Application {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Creating my own data types.");
		
		Person akshaya = new Person();
		akshaya.setAge(12);
		akshaya.name = "Akshaya";
		akshaya.grade = 7;

		Person adhira = new Person();
		adhira.setAge(12);
		adhira.name = "Adhira";
		adhira.grade = 7;

		System.out.println(akshaya.name);
		System.out.println(adhira.name);
		System.out.println(akshaya.age);
		System.out.println(adhira.age);
		System.out.println(akshaya.grade);
		System.out.println(adhira.grade);
	}

}
