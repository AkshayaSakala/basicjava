
public class Application {
	public static void main(String[] args) {
	
		String[] text;
		text =new String[5];
		text[0] = "first";
		text[1] = "second";
		text[2] = "third";
		text[3]	= "fourth";		
		text[4] = "fifth";
		System.out.println(text[0] + text[1]); 
		
		String[] array;
		//array = new String[3];
		array =new String[] {"first","second","third"};
		System.out.println(array[1]);
	}
}
