
public class Student {
	String name;
	int age;

	Student(String inputName) {
		name = inputName;
		System.out.println("The Student's name is " + name + " !");
	}

	Student(String inputName, int inputAge) {
		name = inputName;
		age = inputAge;
		System.out.println(" The Student's name is " + name + " ! The Student's age is " + age + " !");
	}
}
