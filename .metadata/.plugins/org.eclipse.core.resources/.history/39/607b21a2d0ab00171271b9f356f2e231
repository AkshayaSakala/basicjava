public class Person {
	private int age;
	private String name;
	private int grade;
	

	
	int getAge(){
		return age;
	}
	String getName(){
		return name;
	}
	int getGrade(){
		return grade;
	}

	
	Person(int years, String letters, int standard){
		age= years;
		name= letters;
		grade= standard;
		
	}
}


